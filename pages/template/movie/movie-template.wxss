@import '/pages/template/stars/stars.wxss';
.movie-container{
    display: flex;
    flex-direction: column;
    padding:0 22rpx;
}
.movie-img{
    width: 200rpx;
    height: 200rpx;
    padding-bottom: 20rpx;
}
.movie-title{
    margin-bottom: 16rpx;
    font-size: 24rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    width: 200rpx;
}